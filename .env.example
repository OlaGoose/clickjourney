# =============================================================================
# AI Provider (location-info API): auto | gemini | doubao | openai，仅服务端
# =============================================================================
AI_PROVIDER=auto

# =============================================================================
# Doubao (location-info, optional)，仅服务端
# =============================================================================
DOUBAO_API_KEY=
DOUBAO_CHAT_MODEL=doubao-seed-1-6-lite-251015
DOUBAO_CHAT_ENDPOINT=https://ark.cn-beijing.volces.com/api/v3/chat/completions

# =============================================================================
# Gemini：全部仅服务端（API 路由 + /api/gemini-client 代理），密钥不要用 NEXT_PUBLIC_
# =============================================================================
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash
GEMINI_MODEL_STT=gemini-2.5-flash
GEMINI_MODEL_CINEMATIC=gemini-3-flash-preview
GEMINI_MODEL_SECTION=gemini-2.5-flash
GEMINI_MODEL_IMAGE=gemini-3-pro-image-preview
GEMINI_MODEL_IMAGE_EDIT=gemini-2.5-flash-image
GEMINI_MODEL_IMAGE_ANALYZE=gemini-3-pro-preview
GEMINI_MODEL_TTS=gemini-2.5-flash-preview-tts

# =============================================================================
# OpenAI (location-info, optional)，仅服务端
# =============================================================================
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# =============================================================================
# Supabase (auth + DB sync)
# =============================================================================
# URL 和 anon key 设计上可公开，用于浏览器端 Auth；RLS 负责安全。
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# =============================================================================
# Google Maps (editor location picker)，前端使用，需在 Google Cloud 限制 HTTP referrer
# =============================================================================
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# =============================================================================
# Google Cloud Storage (optional; /api/upload-media)，仅服务端
# =============================================================================
GCS_BUCKET=
GCS_SERVICE_ACCOUNT_JSON=

# =============================================================================
# Auth / demo / dev flags
# =============================================================================
SKIP_AUTH_FOR_API=

NEXT_PUBLIC_ENABLE_MOCK_AUTH=
NEXT_PUBLIC_ALLOW_MOCK_IN_PRODUCTION=
NEXT_PUBLIC_ENABLE_DEMO_DATA=

# Supabase
PASS_WORD=
